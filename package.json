{
  "name": "@tinijs/parcel-reporter-copy-public",
  "version": "0.1.0",
  "description": "A ParcelJS reporter for copying the public folder.",
  "author": "Lam Nhan",
  "homepage": "https://lamnhan.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/index.js",
  "source": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src",
    "!**/*.ts",
    "**/*.d.ts"
  ],
  "engines": {
    "node": ">= 12.0.0",
    "parcel": "^2.9.3"
  },
  "scripts": {
    "compile": "tsc",
    "build": "npm run compile",
    "deploy": "npm run build && npm publish",
    "lint": "gts lint",
    "fix": "gts fix"
  },
  "keywords": [
    "node",
    "module",
    "parceljs",
    "tinijs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tinijs/parcel-reporter-copy-public.git"
  },
  "bugs": {
    "url": "https://github.com/tinijs/parcel-reporter-copy-public/issues"
  },
  "dependencies": {
    "@parcel/plugin": "^2.9.3",
    "@parcel/types": "^2.9.3"
  },
  "devDependencies": {
    "@types/node": "^20.4.3",
    "gts": "^4.0.1",
    "typescript": "~5.1.6"
  }
}
